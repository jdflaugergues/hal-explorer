{
  "name": "hal-explorer",
  "version": "1.0.0",
  "description": "HAL Explorer",
  "author": "Jonathan de Flaugergues <jdflaugergues@gmail.com>",
  "private": true,
  "main": "./server/index.js",
  "scripts": {
    "postinstall": "if test \"$NODE_ENV\" = \"production\" ; then npm run build ; fi ",
    "start:server": "nodemon . | npx bunyan",
    "start": "concurrently --kill-others \"npm run start:server\" \"npm run serve\"",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@koa/cors": "2.2.3",
    "ajv": "6.10.2",
    "axios": "0.19.0",
    "bson": "4.0.2",
    "bunyan": "1.8.12",
    "config": "3.2.3",
    "halson": "3.0.0",
    "js-yaml": "3.13.1",
    "koa": "2.10.0",
    "koa-body": "4.1.1",
    "koa-router": "7.4.0",
    "koa-static": "5.0.0",
    "lodash": "4.17.15",
    "marked": "4.0.10",
    "uuid": "3.3.3",
    "vue": "2.6.10",
    "vue-axios": "2.1.5",
    "vue-material": "1.0.0-beta-11",
    "vue-resource": "1.5.1",
    "vue-router": "3.1.3",
    "vuelidate": "0.7.4",
    "vuex": "3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.12.0",
    "@vue/cli-plugin-eslint": "3.12.0",
    "@vue/cli-service": "3.12.0",
    "babel-eslint": "10.0.1",
    "concurrently": "5.0.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "6.4.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-vue": "5.0.0",
    "nodemon": "1.19.3",
    "vue-template-compiler": "2.6.10"
  },
  "engines": {
    "node": "11.15.0",
    "npm": "6.12.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "plugin:prettier/recommended",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
