{
  "name": "hal-explorer",
  "version": "1.0.0",
  "description": "HAL Explorer",
  "author": "Jonathan de Flaugergues <jdflaugergues@gmail.com>",
  "private": true,
  "main": "./server/index.js",
  "scripts": {
    "postinstall": "if test \"$NODE_ENV\" = \"production\" ; then npm run build ; fi ",
    "start": "node .",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "0.19.0",
    "express": "4.17.1",
    "serve-static": "1.14.1",
    "vue": "2.6.10",
    "vue-axios": "2.1.5",
    "vue-material": "1.0.0-beta-11",
    "vue-resource": "1.5.1",
    "vue-router": "3.1.3",
    "vuex": "3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.12.0",
    "@vue/cli-plugin-eslint": "3.12.0",
    "@vue/cli-service": "3.12.0",
    "babel-eslint": "10.0.1",
    "eslint": "5.16.0",
    "eslint-plugin-vue": "5.0.0",
    "vue-template-compiler": "2.6.10"
  },
  "engines": {
    "node": "11.15.0",
    "npm": "6.7.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
